    ' ======================== Getting settings ===================
        if not isError then
            try 
                dim vmixAssemblyTemp As Reflection.Assembly = Reflection.Assembly.GetEntryAssembly()            
                dim mySettingsTypeName as string = "vMix" &amp; "." &amp; "My" &amp; "." &amp; "MySettings"
