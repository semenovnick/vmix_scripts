                    console.wl("DEBUG (line " &amp; me.CurrentLine.tostring() &amp; "): Multicorder settings loaded successfully")
                    isError = false
                else 
                    isError = true
                    console.wl("ERROR (line " &amp; me.CurrentLine.tostring() &amp; "): currentSettings.MultiCorderSettings is NOTHING.")
